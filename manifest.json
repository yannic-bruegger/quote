{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "id": "/",
  "name": "Quote",
  "short_name": "Quote",
  "scope_url": "http://localhost:5500/",
  "intent_filters": {
    "scope_url_scheme": "https",
    "scope_url_host": "quoteapp.de",
    "scope_url_path": "/"
  },
  "start_url": "http://localhost:5500/",
  "display": "standalone",
  "background_color": "#f6f6f9",
  "description": "An app to store quotes in collections and moderate them with your peers.",
  "icons": [{
    "src": "assets/app-images/android/android-launchericon-192-192.png",
    "sizes": "192x192",
    "type": "image/png"
  }, {
    "src": "assets/app-images/android/android-launchericon-512-512.png",
    "sizes": "512x512",
    "type": "image/png"
  }],
  "protocol_handlers": [
    {
      "protocol": "web+collection",
      "url": "/collection?id=%s"
    }
  ]
}
